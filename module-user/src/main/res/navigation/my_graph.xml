<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_graph"
    tools:ignore="InvalidNavigation">
    <fragment
        android:id="@+id/coin"
        android:name="com.example.fragment.module.user.fragment.MyCoinFragment"
        android:label="MyCoinFragment"
        tools:layout="@layout/my_coin_fragment">
        <!--
            app:uri 通过 uri 跳转目的导航图
        -->
        <deepLink app:uri="fragment.example.com/my/coin" />
    </fragment>
    <fragment
        android:id="@+id/coin_rank"
        android:name="com.example.fragment.module.user.fragment.CoinRankFragment"
        android:label="CoinRankFragment"
        tools:layout="@layout/coin_rank_fragment">
        <deepLink app:uri="fragment.example.com/coin/rank" />
    </fragment>
    <fragment
        android:id="@+id/my_collect"
        android:name="com.example.fragment.module.user.fragment.MyCollectFragment"
        android:label="MyCollectFragment"
        tools:layout="@layout/my_collect_fragment">
        <deepLink app:uri="fragment.example.com/my/collect" />
    </fragment>
    <fragment
        android:id="@+id/my_share"
        android:name="com.example.fragment.module.user.fragment.MyShareFragment"
        android:label="MyShareFragment"
        tools:layout="@layout/my_share_fragment">
        <deepLink app:uri="fragment.example.com/my/share" />
    </fragment>
</navigation>